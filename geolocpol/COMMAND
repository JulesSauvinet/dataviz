topojson -o regionswithprop.json --shapefile-encoding utf-8 --id-property code_insee --properties nom=nom --properties nom_cl=nom_cl --properties surf_km2=surf_km2 regions.json

ogr2ogr \
  -f GeoJSON \
  //-where "ADM0_A3 IN ('GBR', 'IRL')" \
  subunits.json \
  ne_10m_admin_0_map_subunits.shp

ogr2ogr -f GeoJSON coursdeau.json COURS_D_EAU.shp

ogr2ogr zre.json ZRE.shp


topojson -o zre2.json --shapefile-encoding utf-8 --id-property CodeNatZon --properties nom=NomZone zre.json